  - key: DistanceFunction
    value: "        #include \"Common.cginc\"\n\n        float3 _EvilRepeat;\n       
      float _IfsLoop;\n        float4 _EvilOffset;\n        float _RotateXY1;\n       
      float _RotateXZ1;\n        float _RotateYZ1;\n        float3 _EvilBoxSize;\n\n       
      inline float DistanceFunction(float3 p)\n        {\n            // \u5E8A\n           
      p = Repeat(p, _EvilRepeat);\n\n            for (int i = 0; i < _IfsLoop; i++)\n           
      {\n                // p -= _EvilOffset;\n                p.xy = abs(p.xy);\n               
      p -= _EvilOffset;\n                p.xy = mul(rotate(_RotateXY1), p.xy);\n               
      p.xz = mul(rotate(_RotateXZ1), p.xz);\n                p.yz = mul(rotate(_RotateYZ1),
      p.yz);\n                \n                // p *= 1.1;\n            }\n\n           
      float3 size = _EvilBoxSize;\n            size.x += _AudioSpectrumLevels[0]
      * 50;\n            float d = sdBox(p, size);\n\n            return d;\n       
      }"
    folded: 0
